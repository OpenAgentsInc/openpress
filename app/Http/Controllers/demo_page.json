{
  "title": "The Atlantis Port Company",
  "slug": "welcome",
  "theme": {
    "background-color": "#242A4C",
    "color": "#FFFFFF"
  },
  "containerClasses": [
    "h-screen",
    "fixed",
    "w-full",
    "overflow-auto",
    "uppercase",
    "flex flex-col justify-center items-center"
  ],
  "blocks": [
    {
      "type": "Container",
      "id": "main-container",
      "attributes": {
        "className": "page-container",
        "style": {
          "background-color": "{{theme.background-color}}",
          "color": "{{theme.color}}",
          "font-family": "'Berkeley Mono', monospace"
        }
      },
      "children": [
        {
          "type": "Image",
          "id": "hero-image",
          "attributes": {
            "src": "images/tapc.png",
            "alt": "OpenPress Hero Image",
            "width": 700,
            "height": 600
          }
        }
      ]
    }
  ]
}
